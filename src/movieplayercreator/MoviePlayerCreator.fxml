<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: gray;"
  prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171"
  fx:controller="movieplayercreator.MoviePlayerCreator" fx:id="root">
  <children>
    <GridPane AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
      AnchorPane.topAnchor="5.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <AnchorPane style="-fx-background-color: black;" GridPane.columnSpan="2" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <Label AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" alignment="CENTER"
              text="Create New Movie Player" textFill="WHITE" wrapText="true">
              <font>
                <Font size="18.0"/>
              </font>
            </Label>
            <Label AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" alignment="BOTTOM_CENTER"
              textFill="RED" fx:id="output">
              <font>
                <Font size="14.0"/>
              </font>
            </Label>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="2" prefHeight="200.0" prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0" layoutX="-27.0"
              layoutY="37.0" promptText="Product Name" fx:id="productName"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="3" prefHeight="200.0" prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0" layoutX="-22.0"
              layoutY="31.0" promptText="Resolution" fx:id="resolution"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.rowIndex="1" prefHeight="200.0" prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0" layoutX="-16.0"
              layoutY="37.0" promptText="Refresh Rate" fx:id="refreshRate"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0" layoutX="-19.0"
              layoutY="50.0" promptText="Response Time" fx:id="responseTime"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0" layoutX="-33.0"
              layoutY="23.0" promptText="Monitor Type" fx:id="monitorType"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="3" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" layoutX="-16.0"
              layoutY="14.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
              </rowConstraints>
              <children>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                  <children>
                    <TextField AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="5.0"
                      AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="10.0" promptText="Quantity"
                      fx:id="quantity"/>
                  </children>
                </AnchorPane>
                <AnchorPane GridPane.rowIndex="1" prefHeight="200.0" prefWidth="200.0">
                  <children>
                    <Button AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0"
                      AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0"
                      mnemonicParsing="false" onAction="#onCreatePressed" text="Create"/>
                  </children>
                </AnchorPane>
              </children>
            </GridPane>
          </children>
        </AnchorPane>
      </children>
    </GridPane>
  </children>
</AnchorPane>
