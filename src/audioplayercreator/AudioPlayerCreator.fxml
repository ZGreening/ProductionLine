<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: grey;"
  prefHeight="120.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/8.0.171"
  fx:controller="audioplayercreator.AudioPlayerCreator" fx:id="root">
  <children>
    <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
      AnchorPane.topAnchor="0.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <AnchorPane GridPane.columnSpan="4" prefHeight="200.0" prefWidth="200.0">
          <children>
            <Label style="-fx-background-color: black;" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
              alignment="TOP_CENTER" layoutX="102.0" layoutY="14.0" text="Create New Audio Player"
              textFill="WHITE">
              <font>
                <Font name="System Bold" size="18.0"/>
              </font>
            </Label>
            <Label AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" alignment="BOTTOM_CENTER"
              textFill="RED" fx:id="output">
              <font>
                <Font size="14.0"/>
              </font>
            </Label>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.rowIndex="1" prefHeight="200.0" prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="35.0" focusTraversable="false"
              promptText="Name" fx:id="productName"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="35.0" focusTraversable="false"
              promptText="Specification" fx:id="productSpecification"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <TextField AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="35.0" focusTraversable="false"
              promptText="Quantity" fx:id="quantity"/>
          </children>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="3" GridPane.rowIndex="1" prefHeight="200.0"
          prefWidth="200.0">
          <children>
            <Button AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" layoutX="34.0"
              layoutY="22.0" mnemonicParsing="false" onAction="#onCreatePressed" text="Create"/>
          </children>
        </AnchorPane>
      </children>
    </GridPane>
  </children>
</AnchorPane>
